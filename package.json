{
  "name": "js-spotify-api",
  "version": "0.0.0-development",
  "engines": {
    "node": ">=6.0.0"
  },
  "description": "A javascript wrapper for Spotify's Web API",
  "main": "index.js",
  "scripts": {
    "clear": "rimraf lib",
    "clean:docs": "rimraf docs",
    "build": "npm run clear && babel --out-dir lib src",
    "build:watch": "npm run build -- --watch",
    "build:api-references": "documentation readme src/** -f md --section=API",
    "build:docs": "npm run clean:docs && npm run build:api-references && jsdoc -c conf.json -R README.md",
    "build:umd": "webpack --output-filename js-spotify-api.umd.js",
    "build:umd:min": "webpack --output-filename js-spotify-api.umd.min.js -p",
    "build:umd:watch": "npm run build:umd -- --watch",
    "build:all": "npm run build:docs && npm run build && npm run build:umd && npm run build:umd:min",
    "lint": "eslint ./src/**/*.js",
    "watch:lint": "esw -w --cache --clear --color",
    "test": "mocha src/**/*.spec.js --require babel-register --require babel-polyfill --require test/configuration.js",
    "test:tdd": "npm test -- -w",
    "test:coverage": "nyc npm test",
    "check:coverage": "nyc check-coverage --lines 75 --functions 75 --branches 75 --statements 75",
    "coverage": "npm run test:coverage && nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "commit": "npx git-cz",
    "deploy": "npm publish",
    "prepush": " npm run test:coverage && npm run lint",
    "prepublish": "npm run build:all",
    "travis-deploy-once": " npx travis-deploy-once",
    "semantic-release": "npx semantic-release"
  },
  "files": [
    "lib",
    "dist"
  ],
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rubengomex/js-spotify-api.git"
  },
  "keywords": [
    "js",
    "library",
    "promises",
    "tdd",
    "spotify",
    "api"
  ],
  "author": "RÃºben Gomes <gomesruben21@gmail.com> (https://rubengomex.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rubengomex/js-spotify-api/issues"
  },
  "homepage": "https://github.com/rubengomex/js-spotify-api#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.0.4",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "documentation": "^8.1.0",
    "eslint": "^5.3.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-mocha": "^5.1.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-watch": "^4.0.2",
    "husky": "^0.14.3",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "opener": "^1.4.3",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.8",
    "sinon": "^6.1.4",
    "sinon-chai": "^3.2.0",
    "travis-deploy-once": "^5.0.2",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}
